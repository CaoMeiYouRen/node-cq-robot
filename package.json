{
  "name": "cq-robot",
  "version": "1.1.11",
  "description": "基于http-api和cq-websocket封装的酷Q机器人SDK",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "CoolQ",
    "CQ",
    "QQRobot"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -d",
    "dev": "tsc -w",
    "lint": "eslint src --ext .ts",
    "test": "mocha -r ts-node/register test/**/*.test.ts -R nyan",
    "deploy": "npm run test && npm run build && npm run docs && npm publish",
    "rm": "rimraf node_modules",
    "docs": "npm run doc && npm run webdocs",
    "doc": "rimraf docs && gulp typedoc",
    "webdocs": "rimraf webdocs && gulp webdocs",
    "commit": "git add . && git cz",
    "push": "npm run mit && git push",
    "changelog": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -p",
    "release": "release-it"
  },
  "author": "CaoMeiYouRen",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/CaoMeiYouRen/node-cq-robot",
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/node-cq-robot/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CaoMeiYouRen/node-cq-robot.git"
  },
  "dependencies": {
    "colors": "^1.3.3",
    "cq-websocket": "git+https://github.com/CaoMeiYouRen/node-cq-websocket.git",
    "json5": "^2.1.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@types/chai": "^4.2.8",
    "@types/json5": "^0.0.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.4.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "conventional-changelog-cmyr-config": "^1.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-alloy": "^2.0.4",
    "eslint-plugin-typescript": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-typedoc": "^2.2.4",
    "husky": "^3.0.1",
    "mocha": "^7.1.2",
    "nyc": "^14.1.1",
    "release-it": "^12.4.3",
    "should": "^13.2.3",
    "tern": "^0.24.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.7.5",
    "typescript-eslint-parser": "^22.0.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "changelog": {
    "language": "zh"
  }
}